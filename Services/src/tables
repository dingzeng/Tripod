branch_group				店组
	id						自动编码
	name					名称

branch						机构
	id						机构编码
	parent_id				上级机构编码
	name					机构名称
	short_name				机构简称
	type					机构类型（0-总部、1-区域中心、2-配送中心、3-自营店、4-加盟店）
	contacts_name			联系人
	contacts_mobile			移动电话
	contacts_tel			固定电话
	contacts_email			Email
	address					地址
	gift_store_id			默认赠送仓库
	return_store_id			默认退货仓库
	purchase_store_id		默认进货仓库
	memo					备注
	create_oper_id			创建人
	create_oper_name		
	create_time				创建时间
	last_update_time		最后修改时间

store						仓库
	id						编码
	branch_id				所属机构编码
	name					仓库名称
	is_usable				是否可用

supplier_region				供应商区域
	id						自增编码
	name					区域名称

supplier					供应商
	id						编码
	name					名称
	region_id				所属区域id
	sell_way				经营方式（0-购销、1-联营、2-代销、3-租赁）
	settle_way				结算方式（0-临时指定、1-货到付款、2-指定账期、3-指定日期）
	settle_days				结算周期天数
	settle_date				结算日期
	status					状态（0-正常、9-停用）
	contacts_name			联系人
	contacts_mobile			移动电话
	contacts_tel			固定电话
	contacts_email			Email
	account_bank			开户行
	account_no				账号
	tax_registration_no		税务登记号
	business_license_o		营业执照号
	memo					备注
	create_oper_id			创建人
	create_oper_name		
	create_time				创建时间
	last_update_time		最后修改时间

item_cls					商品类别
	id
	parent_id
	name

item_brand					商品品牌
	id
	name

item_unit					商品单位
	id
	name

item_department				商品部门
	id
	name

item						商品档案
	id

