
supplier					供应商
	id						编码
	name					名称
	region_id				所属区域id
	sell_way				经营方式（0-购销、1-联营、2-代销、3-租赁）
	settle_way				结算方式（0-临时指定、1-货到付款、2-指定账期、3-指定日期）
	settle_days				结算周期天数
	settle_date				结算日期
	status					状态（0-正常、9-停用）
	contacts_name			联系人
	contacts_mobile			移动电话
	contacts_tel			固定电话
	contacts_email			Email
	account_bank			开户行
	account_no				账号
	tax_registration_no		税务登记号
	business_license_o		营业执照号
	memo					备注
	create_oper_id			创建人
	create_oper_name		
	create_time				创建时间
	last_update_time		最后修改时间

item_cls					商品类别
	id
	parent_id
	name

item_brand					商品品牌
	id
	name

item_unit					商品单位
	id
	name

item_department				商品部门
	id
	name

item						商品档案
	id
    name
    item_cls_id
